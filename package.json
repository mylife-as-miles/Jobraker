{
  "version": "1.0.0",
  "source": "./index.html",
  "type": "module",
  "name": "jobraker",
  "description": "A job app",
  "scripts": {
    "dev": "vite",
  "build": "vite build",
  "supabase:start": "cd backend/supabase && npx supabase start",
  "supabase:stop": "cd backend/supabase && npx supabase stop",
  "supabase:status": "cd backend/supabase && npx supabase status",
  "supabase:reset": "cd backend/supabase && npx supabase db reset --local --yes",
  "supabase:diff": "bash -lc 'cd backend/supabase && npx supabase db diff -f $(date +%Y%m%d%H%M%S) --local'",
  "supabase:schema:dump": "cd backend && npx supabase db dump --local --schema public --file ./supabase/schema.sql",
  "supabase:schema:sync": "bash backend/supabase/scripts/sync-schema.sh"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.1",
    "framer-motion": "^10.16.16",
    "gsap": "^3.13.0",
    "lucide-react": "^0.453.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "2.5.4"
  },
  "devDependencies": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@vitejs/plugin-react": "4.3.4",
    "esbuild": "^0.20.0",
    "globals": "15.12.0",
    "supabase": "^2.33.9",
    "tailwindcss": "3.4.16",
    "vite": "^5.4.19"
  },
  "alias": {
    "@/*": "./src/components/ui/$1"
  }
}
